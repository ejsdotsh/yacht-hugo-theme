  },
  "dependencies": {},
  "description": "YACHT Another Custom Hugo Theme",
  "devDependencies": {
    "@catppuccin/tailwindcss": ">=0.1.6",
    "@tailwindcss/typography": ">=0.5.13",
    "npm-run-all": ">=4.1.5",
    "tailwindcss": ">=3.4.4"
  },
  "name": "YACHT",
  "private": true,
  "scripts": {
    "build": "run-s build-*",
    "build-css": "npx tailwindcss -i ./assets/css/main.css -o ./assets/css/yacht.css -m",
    "build-hugo": "hugo --cleanDestinationDir --minify",
    "dev": "run-p dev:*",
    "dev:css": "npx tailwindcss -i ./assets/css/main.css -o ./assets/css/yacht.css -w",
    "dev:hugo": "hugo server --cleanDestinationDir --disableFastRender --logLevel debug -s exampleSite"
  },
  "version": "0.0.11"
}
